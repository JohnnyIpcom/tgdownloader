    "version": "0.2.0",
    "configurations": [
        {
            "name": "Version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "version",
                "--config",
                "${workspaceFolder}/tgdownloader.yaml"
            ]
        },
        {
            "name": "Peer List",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "peer",
                "list"
            ]
        },
        {
            "name": "Download",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "download",
                "--config",
                "${workspaceFolder}/tgdownloader.yaml",
                "--id",
                "1745633015",
                "--output",
                "${workspaceFolder}/downloads",
                "--limit",
                "25"
            ]
        }
    ]
}